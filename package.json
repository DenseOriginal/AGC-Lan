{
  "name": "ag-lan-website",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev:server": "tsnd --respawn src/server.ts --project tsconfig.server.json",
    "dev:client": "vite build --watch --mode development",
    "build:server": "npm run clean:server && tsc --project tsconfig.server.json",
    "build:client": "npm run clean:client && tsc --project tsconfig.client.json && vite build",
    "clean:server": "rimraf dist/server/",
    "clean:client": "rimraf dist/client/",
    "build": "npm run build:server && npm run build:client",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "preview": "vite preview",
    "build:serve": "npm run build && npm run serve",
    "serve": "node ./dist/server.js"
  },
  "engines": {
    "node": "16.x"
  },
